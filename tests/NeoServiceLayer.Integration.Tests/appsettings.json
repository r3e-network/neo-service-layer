{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "InMemory"
  },
  "Redis": {
    "ConnectionString": "redis:6379"
  },
  "Tee": {
    "SimulationMode": true,
    "EnclaveLibraryPath": "neoservicelayer_enclave.so"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://neoservices.io",
      "http://localhost:3000"
    ]
  },
  "Authentication": {
    "JwtBearer": {
      "Authority": "https://auth.neoservices.io",
      "Audience": "neoservices-api"
    }
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "Window": 60,
    "QueueLimit": 10
  }
}
