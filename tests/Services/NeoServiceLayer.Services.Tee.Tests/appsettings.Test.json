{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SGX": {
    "SimulationMode": true,
    "AttestationServiceUrl": "https://api.trustedservices.intel.com/sgx/certification/v4/",
    "StoragePath": "TestData/SGX",
    "MaxThreads": 4,
    "NetworkTimeoutSeconds": 30,
    "CryptoAlgorithms": [
      "aes-256-gcm",
      "ed25519",
      "secp256k1",
      "sha256"
    ],
    "EnableAI": true,
    "EnableOracle": true,
    "Computation": {
      "MaxConcurrentJobs": 5,
      "TimeoutSeconds": 30,
      "MemoryLimitMB": 64,
      "CodeSizeLimitMB": 1,
      "ParameterSizeLimitKB": 10
    },
    "Storage": {
      "MaxFileSizeMB": 100,
      "EnableCompression": true,
      "DefaultCompressionType": "lz4",
      "EncryptionRequired": true
    },
    "Security": {
      "MinSecurityVersion": 1,
      "AllowDebugMode": true,
      "RequiredSigners": [
        "Intel Corporation"
      ],
      "QuoteMaxAgeHours": 24,
      "CertificateValidationEnabled": true
    }
  },
  "TestSettings": {
    "TempStoragePath": "TestData/Temp",
    "MockServices": true,
    "EnablePerformanceTests": true,
    "ConcurrentTestThreads": 20,
    "LoadTestIterations": 100
  }
}