@page
@model IndexModel
@{
    ViewData["Title"] = "Neo Service Layer";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Interactive Web Interface</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <style>
        .service-card {
            transition: all 0.3s ease;
            border: 1px solid #dee2e6;
            border-radius: 10px;
            background: linear-gradient(135deg, #f8f9fa 0%, #ffffff 100%);
        }
        .service-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 25px rgba(0,0,0,0.1);
            border-color: #007bff;
        }
        .feature-icon {
            font-size: 3rem;
            margin-bottom: 1rem;
            color: #007bff;
        }
        .nav-pills .nav-link.active {
            background-color: #007bff;
        }
        .result-container {
            background-color: #f8f9fa;
            border-radius: 8px;
            border: 1px solid #dee2e6;
        }
        .spinner-border-sm {
            width: 1rem;
            height: 1rem;
        }
        .response-json {
            background-color: #2d3748;
            color: #e2e8f0;
            border-radius: 8px;
            font-family: 'Courier New', monospace;
        }
        .header-gradient {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
        }
        .section-title {
            color: #495057;
            border-bottom: 2px solid #007bff;
            padding-bottom: 0.5rem;
            margin-bottom: 1.5rem;
        }
        .nav-header {
            font-size: 0.75rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.5px;
            border-bottom: 1px solid #dee2e6;
            padding-bottom: 0.25rem;
        }
        .nav-pills .nav-link {
            font-size: 0.875rem;
            padding: 0.5rem 0.75rem;
            margin-bottom: 0.25rem;
            border-radius: 6px;
        }
        .nav-pills .nav-link i {
            width: 16px;
            text-align: center;
        }
        .feature-icon.small {
            font-size: 1.5rem;
        }
    </style>
</head>
<body>
    <!-- Header -->
    <div class="header-gradient py-4 mb-4">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-8">
                    <h1 class="display-4 mb-0">
                        <i class="fas fa-cube me-3"></i>Neo Service Layer
                    </h1>
                    <p class="lead mb-0">Interactive Web Interface for Blockchain Services</p>
                </div>
                <div class="col-md-4 text-end">
                    <div class="d-flex align-items-center justify-content-end">
                        <span id="connection-status" class="badge bg-warning me-3">
                            <i class="fas fa-circle"></i> Connecting...
                        </span>
                        <a href="/services" class="btn btn-success btn-sm me-2">
                            <i class="fas fa-layer-group"></i> View All Services
                        </a>
                        <button id="authBtn" class="btn btn-light btn-sm">
                            <i class="fas fa-key"></i> Get Demo Token
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <!-- Navigation Sidebar -->
            <div class="col-md-3 col-lg-2">
                <div class="nav flex-column nav-pills sticky-top" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                    <button class="nav-link active" id="v-pills-overview-tab" data-bs-toggle="pill" data-bs-target="#v-pills-overview" type="button" role="tab">
                        <i class="fas fa-home me-2"></i>Overview
                    </button>
                    
                    <!-- Foundation Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Foundation Layer</h6>
                    <button class="nav-link" id="v-pills-keymanagement-tab" data-bs-toggle="pill" data-bs-target="#v-pills-keymanagement" type="button" role="tab">
                        <i class="fas fa-key me-2"></i>Key Management
                    </button>
                    <button class="nav-link" id="v-pills-enclave-tab" data-bs-toggle="pill" data-bs-target="#v-pills-enclave" type="button" role="tab">
                        <i class="fas fa-shield-alt me-2"></i>SGX Enclave
                    </button>
                    <button class="nav-link" id="v-pills-storage-tab" data-bs-toggle="pill" data-bs-target="#v-pills-storage" type="button" role="tab">
                        <i class="fas fa-database me-2"></i>Storage
                    </button>
                    
                    <!-- Security Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Security Layer</h6>
                    <button class="nav-link" id="v-pills-compliance-tab" data-bs-toggle="pill" data-bs-target="#v-pills-compliance" type="button" role="tab">
                        <i class="fas fa-check-circle me-2"></i>Compliance
                    </button>
                    <button class="nav-link" id="v-pills-zeroknowledge-tab" data-bs-toggle="pill" data-bs-target="#v-pills-zeroknowledge" type="button" role="tab">
                        <i class="fas fa-user-secret me-2"></i>Zero Knowledge
                    </button>
                    <button class="nav-link" id="v-pills-backup-tab" data-bs-toggle="pill" data-bs-target="#v-pills-backup" type="button" role="tab">
                        <i class="fas fa-save me-2"></i>Backup
                    </button>
                    
                    <!-- Intelligence Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Intelligence Layer</h6>
                    <button class="nav-link" id="v-pills-ai-tab" data-bs-toggle="pill" data-bs-target="#v-pills-ai" type="button" role="tab">
                        <i class="fas fa-brain me-2"></i>AI Services
                    </button>
                    <button class="nav-link" id="v-pills-pattern-tab" data-bs-toggle="pill" data-bs-target="#v-pills-pattern" type="button" role="tab">
                        <i class="fas fa-search me-2"></i>Pattern Recognition
                    </button>
                    <button class="nav-link" id="v-pills-oracle-tab" data-bs-toggle="pill" data-bs-target="#v-pills-oracle" type="button" role="tab">
                        <i class="fas fa-eye me-2"></i>Oracle
                    </button>
                    
                    <!-- Blockchain Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Blockchain Layer</h6>
                    <button class="nav-link" id="v-pills-abstractaccount-tab" data-bs-toggle="pill" data-bs-target="#v-pills-abstractaccount" type="button" role="tab">
                        <i class="fas fa-user-cog me-2"></i>Abstract Account
                    </button>
                    <button class="nav-link" id="v-pills-voting-tab" data-bs-toggle="pill" data-bs-target="#v-pills-voting" type="button" role="tab">
                        <i class="fas fa-vote-yea me-2"></i>Voting
                    </button>
                    <button class="nav-link" id="v-pills-crosschain-tab" data-bs-toggle="pill" data-bs-target="#v-pills-crosschain" type="button" role="tab">
                        <i class="fas fa-link me-2"></i>Cross Chain
                    </button>
                    <button class="nav-link" id="v-pills-proofofreserve-tab" data-bs-toggle="pill" data-bs-target="#v-pills-proofofreserve" type="button" role="tab">
                        <i class="fas fa-certificate me-2"></i>Proof of Reserve
                    </button>
                    
                    <!-- Automation Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Automation Layer</h6>
                    <button class="nav-link" id="v-pills-compute-tab" data-bs-toggle="pill" data-bs-target="#v-pills-compute" type="button" role="tab">
                        <i class="fas fa-microchip me-2"></i>Compute
                    </button>
                    <button class="nav-link" id="v-pills-automation-tab" data-bs-toggle="pill" data-bs-target="#v-pills-automation" type="button" role="tab">
                        <i class="fas fa-robot me-2"></i>Automation
                    </button>
                    <button class="nav-link" id="v-pills-notification-tab" data-bs-toggle="pill" data-bs-target="#v-pills-notification" type="button" role="tab">
                        <i class="fas fa-bell me-2"></i>Notification
                    </button>
                    <button class="nav-link" id="v-pills-randomness-tab" data-bs-toggle="pill" data-bs-target="#v-pills-randomness" type="button" role="tab">
                        <i class="fas fa-random me-2"></i>Randomness
                    </button>
                    
                    <!-- Infrastructure Layer -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Infrastructure Layer</h6>
                    <button class="nav-link" id="v-pills-health-tab" data-bs-toggle="pill" data-bs-target="#v-pills-health" type="button" role="tab">
                        <i class="fas fa-heartbeat me-2"></i>Health
                    </button>
                    <button class="nav-link" id="v-pills-monitoring-tab" data-bs-toggle="pill" data-bs-target="#v-pills-monitoring" type="button" role="tab">
                        <i class="fas fa-chart-line me-2"></i>Monitoring
                    </button>
                    <button class="nav-link" id="v-pills-configuration-tab" data-bs-toggle="pill" data-bs-target="#v-pills-configuration" type="button" role="tab">
                        <i class="fas fa-cog me-2"></i>Configuration
                    </button>
                    <button class="nav-link" id="v-pills-events-tab" data-bs-toggle="pill" data-bs-target="#v-pills-events" type="button" role="tab">
                        <i class="fas fa-calendar-alt me-2"></i>Event Subscription
                    </button>
                    
                    <!-- Developer Tools -->
                    <h6 class="nav-header text-muted mt-3 mb-2 px-3">Developer Tools</h6>
                    <button class="nav-link" id="v-pills-tools-tab" data-bs-toggle="pill" data-bs-target="#v-pills-tools" type="button" role="tab">
                        <i class="fas fa-tools me-2"></i>API Testing
                    </button>
                </div>
            </div>

            <!-- Main Content -->
            <div class="col-md-9 col-lg-10">
                <div class="tab-content" id="v-pills-tabContent">
                    <!-- Overview Tab -->
                    <div class="tab-pane fade show active" id="v-pills-overview" role="tabpanel">
                        <h2 class="section-title">Neo Service Layer - Complete Service Overview</h2>
                        
                        <!-- Foundation Layer -->
                        <h3 class="section-title">🔧 Foundation Layer</h3>
                        <div class="row g-4 mb-5">
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-key feature-icon"></i>
                                        <h5 class="card-title">Key Management</h5>
                                        <p class="card-text">Generate, store, and manage cryptographic keys for blockchain operations with enclave security.</p>
                                        <div class="mt-auto">
                                            <span id="keymanagement-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-shield-alt feature-icon"></i>
                                        <h5 class="card-title">SGX Enclave</h5>
                                        <p class="card-text">Secure computation in trusted execution environments with Intel SGX hardware attestation.</p>
                                        <div class="mt-auto">
                                            <span id="sgx-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-database feature-icon"></i>
                                        <h5 class="card-title">Storage</h5>
                                        <p class="card-text">Secure, encrypted, and compressed data storage with transaction support and metadata management.</p>
                                        <div class="mt-auto">
                                            <span id="storage-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Security Layer -->
                        <h3 class="section-title">🔒 Security Layer</h3>
                        <div class="row g-4 mb-5">
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-check-circle feature-icon"></i>
                                        <h5 class="card-title">Compliance</h5>
                                        <p class="card-text">Regulatory compliance monitoring, audit trails, and violation detection with automated reporting.</p>
                                        <div class="mt-auto">
                                            <span id="compliance-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-user-secret feature-icon"></i>
                                        <h5 class="card-title">Zero Knowledge</h5>
                                        <p class="card-text">Privacy-preserving cryptographic proofs and verifications with zk-SNARK support.</p>
                                        <div class="mt-auto">
                                            <span id="zeroknowledge-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-save feature-icon"></i>
                                        <h5 class="card-title">Backup</h5>
                                        <p class="card-text">Automated backup and restore operations with encryption, compression, and integrity verification.</p>
                                        <div class="mt-auto">
                                            <span id="backup-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Intelligence Layer -->
                        <h3 class="section-title">🧠 Intelligence Layer</h3>
                        <div class="row g-4 mb-5">
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-brain feature-icon"></i>
                                        <h5 class="card-title">AI Services</h5>
                                        <p class="card-text">Machine learning predictions, risk assessment, and price forecasting with secure enclave execution.</p>
                                        <div class="mt-auto">
                                            <span id="ai-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-search feature-icon"></i>
                                        <h5 class="card-title">Pattern Recognition</h5>
                                        <p class="card-text">Advanced pattern analysis, anomaly detection, and fraud detection with AI-powered insights.</p>
                                        <div class="mt-auto">
                                            <span id="pattern-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="card service-card h-100 text-center p-4">
                                    <div class="card-body">
                                        <i class="fas fa-eye feature-icon"></i>
                                        <h5 class="card-title">Oracle</h5>
                                        <p class="card-text">External data feeds, price oracles, and secure data aggregation from multiple sources.</p>
                                        <div class="mt-auto">
                                            <span id="oracle-status" class="badge bg-secondary">Checking...</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Blockchain Layer -->
                        <h3 class="section-title">⛓️ Blockchain Layer</h3>
                        <div class="row g-4 mb-5">
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-user-cog feature-icon"></i>
                                        <h6 class="card-title">Abstract Account</h6>
                                        <p class="card-text small">Smart contract wallet management with session keys and multi-signature support.</p>
                                        <span id="abstractaccount-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-vote-yea feature-icon"></i>
                                        <h6 class="card-title">Voting</h6>
                                        <p class="card-text small">Decentralized voting with strategy automation and consensus node management.</p>
                                        <span id="voting-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-link feature-icon"></i>
                                        <h6 class="card-title">Cross Chain</h6>
                                        <p class="card-text small">Inter-blockchain communication and asset bridging with secure validation.</p>
                                        <span id="crosschain-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-certificate feature-icon"></i>
                                        <h6 class="card-title">Proof of Reserve</h6>
                                        <p class="card-text small">Cryptographic proof of asset reserves with periodic attestation and auditing.</p>
                                        <span id="proofofreserve-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Automation Layer -->
                        <h3 class="section-title">🤖 Automation Layer</h3>
                        <div class="row g-4 mb-5">
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-microchip feature-icon"></i>
                                        <h6 class="card-title">Compute</h6>
                                        <p class="card-text small">Distributed computation with resource allocation and secure job execution.</p>
                                        <span id="compute-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-robot feature-icon"></i>
                                        <h6 class="card-title">Automation</h6>
                                        <p class="card-text small">Workflow automation and task scheduling with trigger-based execution.</p>
                                        <span id="automation-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-bell feature-icon"></i>
                                        <h6 class="card-title">Notification</h6>
                                        <p class="card-text small">Multi-channel notification system with template support and delivery tracking.</p>
                                        <span id="notification-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-random feature-icon"></i>
                                        <h6 class="card-title">Randomness</h6>
                                        <p class="card-text small">Cryptographically secure random number generation with bias resistance.</p>
                                        <span id="randomness-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Infrastructure Layer -->
                        <h3 class="section-title">🏗️ Infrastructure Layer</h3>
                        <div class="row g-4">
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-heartbeat feature-icon"></i>
                                        <h6 class="card-title">Health</h6>
                                        <p class="card-text small">System health monitoring and diagnostic reporting with alerting capabilities.</p>
                                        <span id="health-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-chart-line feature-icon"></i>
                                        <h6 class="card-title">Monitoring</h6>
                                        <p class="card-text small">Performance metrics collection and analysis with real-time dashboards.</p>
                                        <span id="monitoring-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-cog feature-icon"></i>
                                        <h6 class="card-title">Configuration</h6>
                                        <p class="card-text small">Dynamic configuration management with validation and encrypted storage.</p>
                                        <span id="configuration-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-3">
                                <div class="card service-card h-100 text-center p-3">
                                    <div class="card-body">
                                        <i class="fas fa-calendar-alt feature-icon"></i>
                                        <h6 class="card-title">Events</h6>
                                        <p class="card-text small">Event subscription and real-time notification system with filtering support.</p>
                                        <span id="events-status" class="badge bg-secondary">Checking...</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Services Quick Access -->
                        <div class="row mt-5">
                            <div class="col-12">
                                <div class="card bg-primary text-white">
                                    <div class="card-body">
                                        <div class="row align-items-center">
                                            <div class="col-md-8">
                                                <h3 class="mb-2">
                                                    <i class="fas fa-layer-group me-3"></i>Service Management Center
                                                </h3>
                                                <p class="mb-0">Access all 21 Neo Service Layer services with interactive interfaces, real-time monitoring, and comprehensive management tools.</p>
                                            </div>
                                            <div class="col-md-4 text-end">
                                                <a href="/services" class="btn btn-light btn-lg">
                                                    <i class="fas fa-external-link-alt me-2"></i>Browse All Services
                                                </a>
                                            </div>
                                        </div>
                                        <div class="row mt-3">
                                            <div class="col-md-3">
                                                <div class="text-center">
                                                    <h4>21</h4>
                                                    <small>Total Services</small>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="text-center">
                                                    <h4>6</h4>
                                                    <small>Service Layers</small>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="text-center">
                                                    <h4>100%</h4>
                                                    <small>SGX Protected</small>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="text-center">
                                                    <h4 id="services-online">--</h4>
                                                    <small>Online Now</small>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
