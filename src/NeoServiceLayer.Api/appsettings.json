{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YourSecretKeyHere",
    "Issuer": "NeoServiceLayer",
    "Audience": "NeoServiceLayerClients"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://neoservices.io",
      "http://localhost:3000"
    ]
  },
  "Telemetry": {
    "Jaeger": {
      "AgentHost": "localhost",
      "AgentPort": "6831"
    }
  },
  "Neo": {
    "RpcUrl": "http://localhost:10332",
    "WalletPath": "neo-wallet.json",
    "WalletPassword": "password"
  },
  "Tee": {
    "Type": "Occlum",
    "SimulationMode": true,
    "EnclavePath": "bin/libenclave.so",
    "Debug": true,
    "OcclumInstanceDir": "/occlum_instance",
    "OcclumLogLevel": "info",
    "Occlum": {
      "InstanceDir": "/occlum_instance",
      "LogLevel": "info",
      "NodeJsPath": "/bin/node",
      "TempDir": "/tmp",
      "EnableDebugMode": true,
      "MaxMemoryMB": 1024,
      "MaxThreads": 32,
      "MaxProcesses": 16,
      "MaxExecutionTimeSeconds": 60
    },
    "JavaScriptEngine": {
      "MaxMemoryMB": 512,
      "MaxExecutionTimeMs": 5000,
      "EnableDebugger": false,
      "NodeModulesPath": "/node_modules",
      "AllowedModules": [
        "fs",
        "crypto",
        "path",
        "os",
        "util",
        "buffer",
        "stream",
        "events",
        "assert"
      ]
    },
    "UserSecrets": {
      "MaxSecretsPerUser": 100,
      "MaxSecretSizeBytes": 4096,
      "EncryptionEnabled": true,
      "RotationIntervalDays": 30
    },
    "GasAccounting": {
      "EnableGasAccounting": true,
      "GasLimitPerExecution": 1000000,
      "GasPriceMultiplier": 1.0,
      "GasRefundPercentage": 50
    },
    "Storage": {
      "Provider": "OcclumFileStorage",
      "StoragePath": "/occlum_instance/data",
      "EnableEncryption": true,
      "EncryptionKey": "NeoServiceLayerEncryptionKey12345678901",
      "EnableCompression": true,
      "CompressionLevel": 6,
      "MaxChunkSize": 4194304,
      "EnableCaching": true,
      "CacheSizeBytes": 52428800,
      "EnableAutoFlush": true,
      "AutoFlushIntervalMs": 5000
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=NeoServiceLayer;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
