<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- CVE-2014-9152: False positive for Neo blockchain library (not the vulnerable "neon" package) -->
    <suppress>
        <notes><![CDATA[
        This CVE affects the "neon" package, not the Neo blockchain library.
        The Neo blockchain library is a different project entirely.
        ]]></notes>
        <packageUrl regex="true">^pkg:nuget/Neo.*$</packageUrl>
        <cve>CVE-2014-9152</cve>
    </suppress>
    
    <!-- CVE-2024-43485: System.Text.Json 9.0.1 - No patch available yet -->
    <suppress until="2025-12-31">
        <notes><![CDATA[
        System.Text.Json 9.0.1 is the latest available version.
        Monitoring for patch availability.
        ]]></notes>
        <packageUrl regex="true">^pkg:nuget/System\.Text\.Json@9\.0\.1$</packageUrl>
        <cve>CVE-2024-43485</cve>
    </suppress>
    
</suppressions>